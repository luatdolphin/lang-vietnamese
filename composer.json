{
    "name": "luatdolphin/lang-vietnamese",
    "type": "flarum-extension",
    "description": "Vietnamese language pack.",
    "keywords": ["locale", "vietnam", "vietnamese"],
    "type": "flarum-extension",
    "license": "MIT",
    "authors": [
     {
      "name": "Luat Dolphin",
      "email": "luatdolphin@gmail.com",
      "homepage": "https://www.thiennhaimobi.ga/"
     }
    ],
  "support": {
    "source": "https://github.com/Littlegolden/flarum-lang-japanese",
    "issues": "https://github.com/luatdolphin/lang-vietnamese/issues"
  },
    "require": {
        "flarum/core": "^0.1.0-beta.15"
    },
	"autoload": {
    "psr-4": {
      "luatdolphin\\lang-vietnamese\\": "src/"
    }
  },
    "extra": {
        "branch-alias": {
            "dev-master": "0.1.x-dev"
        },
        "flarum-extension": {
            "title": "Vietnamese",
	    "category": "language",
            "icon": {
                "image": "icon.svg",
                "backgroundColor": "#00247d",
                "backgroundSize": "cover",
                "backgroundPosition": "center"
            }
        },
        "flarum-locale": {
            "code": "vi",
            "title": "Vietnamese"
        }
    }
}
